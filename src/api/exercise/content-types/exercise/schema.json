{
  "kind": "collectionType",
  "collectionName": "exercises",
  "info": {
    "singularName": "exercise",
    "pluralName": "exercises",
    "displayName": "Exercise",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "uid": {
      "type": "uid",
      "required": true
    },
    "statement": {
      "type": "text",
      "required": false
    },
    "codes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::code.code"
    },
    "choices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::choice.choice"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "code",
        "choice"
      ],
      "default": "choice",
      "required": true
    },
    "answer": {
      "type": "text",
      "required": false
    }
  }
}
